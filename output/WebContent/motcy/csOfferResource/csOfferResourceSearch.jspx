                    <datacolumn fieldname="RESOURCE_UNIT_TYPE" type="lookup" lookup="RESOURCE_UNIT_TYPE_FKLP" text="${msg.RESOURCE_UNIT_TYPE}" id="RESOURCE_UNIT_TYPE"/>
                    <datacolumn fieldname="RESOURCE_TELESERVICE" type="lookup" lookup="RESOURCE_TELESERVICE_FKLP" text="${msg.RESOURCE_TELESERVICE}" id="RESOURCE_TELESERVICE"/>
