                <datatable datasource="${sqlExp.data_source}" var="csOfferResource_item" casesensitive="true" table="CS_OFFER_RESOURCE" showrowindex="true" autobind="true" noresults="${msg.NO_RESULTS}" sql="${sql.CS_OFFER_RESOURCE_SEARCH}">
                    <dataparam control="idWC" expression="${sqlExp.CS_OFFER_RESOURCE_ID}"/>
                    <dataparam control="offerIdResourceWC" expression="${sqlExp.CS_OFFER_RESOURCE_OFFER_ID_RESOURCE}"/>
                    <dataparam control="resourceTypeWC" expression="${sqlExp.CS_OFFER_RESOURCE_RESOURCE_TYPE}"/>
