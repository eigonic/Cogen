                    <datacolumn fieldname="INFO5" type="string" text="${msg.INFO5}" id="INFO5"/>
                    <datacolumn fieldname="INFO6" type="string" text="${msg.INFO6}" id="INFO6"/>
                    <datacolumn fieldname="INFO7" type="string" text="${msg.INFO7}" id="INFO7"/>
                    <datacolumn fieldname="INFO8" type="string" text="${msg.INFO8}" id="INFO8"/>
