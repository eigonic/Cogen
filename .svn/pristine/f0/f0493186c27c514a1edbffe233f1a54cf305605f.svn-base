<?xml version="1.0" encoding="UTF-8"?>
<page controller="com.bay.da.web.controller.AssetPhotoEditController" master="/pages/master.jspx">
	<contentHolder id="mainholder">
		<form class="well form-horizontal" action="/pages/assetPhotoEdit.da">
			<ajaxpanel>
                <fieldset>
                    <div class="row">
                        <div class="span6">
                            <div class="control-group">
                                <label for="idWC" class="control-label">${msg.ID}</label>
                                <div class="controls">
                                    <input type="text" value="${assetPhoto.id}" id="idWC"/>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="createdWC" class="control-label">${msg.CREATED}</label>
                                <div class="controls">
                                    <calendar value="${assetPhoto.created}" id="createdWC"/>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="createdByWC" class="control-label">${msg.CREATED_BY}</label>
                                <div class="controls">
                                    <select value="${assetPhoto.createdBy}" id="createdByWC">
                                        <option>----</option>
                                        <datalookup datasource="${sql.data_source}" key="ID" value="NAME" sql="${sql.users_LOOKUP}"/>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="lastUpdateWC" class="control-label">${msg.LAST_UPDATE}</label>
                                <div class="controls">
                                    <calendar value="${assetPhoto.lastUpdate}" id="lastUpdateWC"/>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="updateByWC" class="control-label">${msg.UPDATE_BY}</label>
                                <div class="controls">
                                    <select value="${assetPhoto.updateBy}" id="updateByWC">
                                        <option>----</option>
                                        <datalookup datasource="${sql.data_source}" key="ID" value="NAME" sql="${sql.users_LOOKUP}"/>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="statusWC" class="control-label">${msg.STATUS}</label>
                                <div class="controls">
                                    <input type="text" value="${assetPhoto.status}" id="statusWC"/>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="delByWC" class="control-label">${msg.DEL_BY}</label>
                                <div class="controls">
                                    <select value="${assetPhoto.delBy}" id="delByWC">
                                        <option>----</option>
                                        <datalookup datasource="${sql.data_source}" key="ID" value="NAME" sql="${sql.users_LOOKUP}"/>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="assetIdWC" class="control-label">${msg.ASSET_ID}</label>
                                <div class="controls">
                                    <input type="text" value="${assetPhoto.assetId}" id="assetIdWC"/>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="photoWC" class="control-label">${msg.PHOTO}</label>
                                <div class="controls">
                                    <input type="file" value="${assetPhoto.photo}" id="photoWC"/>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="photoNameWC" class="control-label">${msg.PHOTO_NAME}</label>
                                <div class="controls">
                                    <input type="text" value="${assetPhoto.photoName}" id="photoNameWC"/>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="photoMimeWC" class="control-label">${msg.PHOTO_MIME}</label>
                                <div class="controls">
                                    <input type="text" value="${assetPhoto.photoMime}" id="photoMimeWC"/>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-action">
                        <input type="button" onserverclick="saveBean" value="${msg.SAVE_CAPTION}" class=" btn-primary"/>
                    </div>
                </fieldset>
            </ajaxpanel>
		</form>
	</contentHolder>
</page>
