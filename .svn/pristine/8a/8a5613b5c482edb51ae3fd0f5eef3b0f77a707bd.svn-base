<?xml version="1.0" encoding="UTF-8"?>
<page controller="com.bay.da.web.controller.ContactEditController" master="/pages/master.jspx">
	<contentHolder id="mainholder">
		<form class="well form-horizontal" action="/pages/contactEdit.da">
			<ajaxpanel>
                <fieldset>
                    <div class="row">
                        <div class="span6">
                            <div class="control-group">
                                <label for="idWC" class="control-label">${msg.ID}</label>
                                <div class="controls">
                                    <input type="text" value="${contact.id}" id="idWC"/>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="nameWC" class="control-label">${msg.NAME}</label>
                                <div class="controls">
                                    <input type="text" value="${contact.name}" id="nameWC"/>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="codeWC" class="control-label">${msg.CODE}</label>
                                <div class="controls">
                                    <input type="text" value="${contact.code}" id="codeWC"/>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="phoneWC" class="control-label">${msg.PHONE}</label>
                                <div class="controls">
                                    <input type="text" value="${contact.phone}" id="phoneWC"/>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="phone2WC" class="control-label">${msg.PHONE2}</label>
                                <div class="controls">
                                    <input type="text" value="${contact.phone2}" id="phone2WC"/>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="genderWC" class="control-label">${msg.GENDER}</label>
                                <div class="controls">
                                    <select value="${contact.gender}" id="genderWC">
                                        <option>----</option>
                                        <datalookup datasource="${sql.data_source}" key="ID" value="NAME" sql="${sql.gender_LOOKUP}"/>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="passportNumWC" class="control-label">${msg.PASSPORT_NUM}</label>
                                <div class="controls">
                                    <input type="text" value="${contact.passportNum}" id="passportNumWC"/>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="photoWC" class="control-label">${msg.PHOTO}</label>
                                <div class="controls">
                                    <input type="file" value="${contact.photo}" id="photoWC"/>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="creationDateWC" class="control-label">${msg.CREATION_DATE}</label>
                                <div class="controls">
                                    <calendar value="${contact.creationDate}" id="creationDateWC"/>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="birthDateWC" class="control-label">${msg.BIRTH_DATE}</label>
                                <div class="controls">
                                    <calendar value="${contact.birthDate}" id="birthDateWC"/>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="jobWC" class="control-label">${msg.JOB}</label>
                                <div class="controls">
                                    <input type="text" value="${contact.job}" id="jobWC"/>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="photoMimeWC" class="control-label">${msg.PHOTO_MIME}</label>
                                <div class="controls">
                                    <input type="text" value="${contact.photoMime}" id="photoMimeWC"/>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="deletedWC" class="control-label">${msg.DELETED}</label>
                                <div class="controls">
                                    <input type="text" value="${contact.deleted}" id="deletedWC"/>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="delDateWC" class="control-label">${msg.DEL_DATE}</label>
                                <div class="controls">
                                    <calendar value="${contact.delDate}" id="delDateWC"/>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="delUserWC" class="control-label">${msg.DEL_USER}</label>
                                <div class="controls">
                                    <select value="${contact.delUser}" id="delUserWC">
                                        <option>----</option>
                                        <datalookup datasource="${sql.data_source}" key="ID" value="NAME" sql="${sql.users_LOOKUP}"/>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-action">
                        <input type="button" onserverclick="saveBean" value="${msg.SAVE_CAPTION}" class=" btn-primary"/>
                    </div>
                </fieldset>
            </ajaxpanel>
		</form>
	</contentHolder>
</page>
