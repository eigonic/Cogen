<?xml version="1.0" encoding="UTF-8"?>
<page controller="com.bay.da.web.controller.CompanyEditController" master="/pages/master.jspx">
	<contentHolder id="mainholder">
		<form class="well form-horizontal" action="/pages/companyEdit.da">
			<ajaxpanel>
                <fieldset>
                    <div class="row">
                        <div class="span6">
                            <div class="control-group">
                                <label for="idWC" class="control-label">${msg.ID}</label>
                                <div class="controls">
                                    <input type="text" value="${company.id}" id="idWC"/>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="nameWC" class="control-label">${msg.NAME}</label>
                                <div class="controls">
                                    <input type="text" value="${company.name}" id="nameWC"/>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="countryIdWC" class="control-label">${msg.COUNTRY_ID}</label>
                                <div class="controls">
                                    <select value="${company.countryId}" id="countryIdWC">
                                        <option>----</option>
                                        <datalookup datasource="${sql.data_source}" key="ID" value="NAME" sql="${sql.country_LOOKUP}"/>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-action">
                        <input type="button" onserverclick="saveBean" value="${msg.SAVE_CAPTION}" class=" btn-primary"/>
                    </div>
                </fieldset>
            </ajaxpanel>
		</form>
	</contentHolder>
</page>
