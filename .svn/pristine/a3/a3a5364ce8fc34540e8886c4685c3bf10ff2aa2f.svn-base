<?xml version="1.0" encoding="UTF-8"?>
<page controller="com.bay.da.web.controller.ContactViewController" master="/pages/master.jspx">
	<contentHolder id="mainholder">
		<form class="well form-horizontal" action="/pages/contactView.da">
			<ajaxpanel>
                <fieldset>
                    <div class="row">
                        <div class="span6">
                            <div class="control-group">
                                <label for="idWC" class="control-label">${msg.ID}</label>
                                <div class="controls">
                                    <label for="" class="control-label">${contact.id}</label>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="nameWC" class="control-label">${msg.NAME}</label>
                                <div class="controls">
                                    <label for="" class="control-label">${contact.name}</label>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="codeWC" class="control-label">${msg.CODE}</label>
                                <div class="controls">
                                    <label for="" class="control-label">${contact.code}</label>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="phoneWC" class="control-label">${msg.PHONE}</label>
                                <div class="controls">
                                    <label for="" class="control-label">${contact.phone}</label>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="phone2WC" class="control-label">${msg.PHONE2}</label>
                                <div class="controls">
                                    <label for="" class="control-label">${contact.phone2}</label>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="genderWC" class="control-label">${msg.GENDER}</label>
                                <div class="controls">
                                    <label for="" class="control-label">${contact.gender}</label>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="passportNumWC" class="control-label">${msg.PASSPORT_NUM}</label>
                                <div class="controls">
                                    <label for="" class="control-label">${contact.passportNum}</label>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="photoWC" class="control-label">${msg.PHOTO}</label>
                                <div class="controls">
                                    <label for="" class="control-label">${contact.photo}</label>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="creationDateWC" class="control-label">${msg.CREATION_DATE}</label>
                                <div class="controls">
                                    <label for="" class="control-label">${contact.creationDate}</label>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="birthDateWC" class="control-label">${msg.BIRTH_DATE}</label>
                                <div class="controls">
                                    <label for="" class="control-label">${contact.birthDate}</label>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="jobWC" class="control-label">${msg.JOB}</label>
                                <div class="controls">
                                    <label for="" class="control-label">${contact.job}</label>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="photoMimeWC" class="control-label">${msg.PHOTO_MIME}</label>
                                <div class="controls">
                                    <label for="" class="control-label">${contact.photoMime}</label>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="deletedWC" class="control-label">${msg.DELETED}</label>
                                <div class="controls">
                                    <label for="" class="control-label">${contact.deleted}</label>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="delDateWC" class="control-label">${msg.DEL_DATE}</label>
                                <div class="controls">
                                    <label for="" class="control-label">${contact.delDate}</label>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="delUserWC" class="control-label">${msg.DEL_USER}</label>
                                <div class="controls">
                                    <label for="" class="control-label">${contact.delUser}</label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-action">
                        <a href="/pages/contactEdit.da?id=${contact.id}" class=" btn-primary btn">EDIT_CAPTION</a>
                        <linkcommand confirmation="${msg.CONFIRM_DELETE}" onserverclick="deleteBean" class=" btn-danger btn">
                            <i class="icon-trash icon-white"/> ${msg.DELETE_CAPTION}</linkcommand>
                    </div>
                </fieldset>
            </ajaxpanel>
		</form>
	</contentHolder>
</page>
