package net.sf.cogen.model;

import java.util.ArrayList;

public class DB
{
	ArrayList<DBTable> dbTables = new ArrayList<DBTable>();
	String modelPackage;

	public DB()
	{
	}

	public ArrayList<DBTable> getDbTables()
	{
		return dbTables;
	}

	public void setDbTables(ArrayList<DBTable> dbTables)
	{
		this.dbTables = dbTables;
	}

	public String getModelPackage()
	{
		return modelPackage;
	}

	public void setModelPackage(String name)
	{
		this.modelPackage = name;
	}

}
