<?xml version="1.0" encoding="UTF-8"?>
<page controller="com.bay.da.web.controller.ActionLogEditController" master="/pages/master.jspx">
	<contentHolder id="mainholder">
		<form class="well form-horizontal" action="/pages/actionLogEdit.da">
			<ajaxpanel>
                <fieldset>
                    <div class="row">
                        <div class="span6">
                            <div class="control-group">
                                <label for="idWC" class="control-label">${msg.ID}</label>
                                <div class="controls">
                                    <input type="text" value="${actionLog.id}" id="idWC"/>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="userIdWC" class="control-label">${msg.USER_ID}</label>
                                <div class="controls">
                                    <input type="text" value="${actionLog.userId}" id="userIdWC"/>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="actionDateWC" class="control-label">${msg.ACTION_DATE}</label>
                                <div class="controls">
                                    <calendar value="${actionLog.actionDate}" id="actionDateWC"/>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label for="actionWC" class="control-label">${msg.ACTION}</label>
                                <div class="controls">
                                    <input type="text" value="${actionLog.action}" id="actionWC"/>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-action">
                        <input type="button" onserverclick="saveBean" value="${msg.SAVE_CAPTION}" class=" btn-primary"/>
                    </div>
                </fieldset>
            </ajaxpanel>
		</form>
	</contentHolder>
</page>
